{
	"name": "katacut",
	"version": "0.0.0",
	"private": true,
	"packageManager": "pnpm@9.9.0",
	"scripts": {
		"build": "pnpm exec tsc -b",
		"clean": "pnpm -r exec rimraf dist",
		"lint": "pnpm -r lint",
		"lint:fix": "biome check --write --unsafe .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:changed": "vitest --changed --changedSince=origin/main",
		"typecheck": "pnpm exec tsc -b",
		"typecheck:watch": "pnpm exec tsc -b --watch",
		"dev": "pnpm exec tsc -b --watch",
		"docs:dev": "pnpm -C packages/docs docs:dev"
	},
	"dependencies": {
		"katacut": "workspace:*"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.6",
		"@teunlao/code-consolidator": "^0.13.0",
		"@types/node": "^24.8.1",
		"rimraf": "^6.0.1",
		"typescript": "^5.9.3",
		"vitepress": "^1.6.4",
		"vitest": "^3.2.4"
	}
}
