{
	"name": "@katacut/cli",
	"version": "0.0.0",
	"private": false,
	"type": "module",
	"bin": {
		"katacut": "./bin/katacut.mjs",
		"ktc": "./bin/katacut.mjs"
	},
	"exports": {
		".": "./dist/index.js",
		"./bin/katacut.mjs": "./bin/katacut.mjs",
		"./bin/*": "./bin/*"
	},
	"types": "./dist/index.d.ts",
	"files": [
		"bin",
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsc --build tsconfig.build.json",
		"lint": "biome lint .",
		"test": "vitest",
		"typecheck": "tsc --project tsconfig.json --noEmit",
		"dev": "tsc --build tsconfig.build.json --watch"
	},
	"dependencies": {
		"@katacut/adapter-client-claude-code": "workspace:*",
		"@katacut/adapter-client-gemini-cli": "workspace:*",
		"@katacut/core": "workspace:*",
		"@katacut/schema": "workspace:*",
		"@katacut/utils": "workspace:*",
		"commander": "^14.0.1",
		"semver": "^7.7.3"
	},
	"devDependencies": {
		"@types/semver": "^7.5.8"
	}
}
